{
    "buildCommand": "npm run build",
    "outputDirectory": "build",
    "framework": "create-react-app",
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/index.html"
      }
    ],
    "env": {
      "REACT_APP_SUPABASE_URL": "@supabase-url",
      "REACT_APP_SUPABASE_ANON_KEY": "@supabase-anon-key"
    }
  }